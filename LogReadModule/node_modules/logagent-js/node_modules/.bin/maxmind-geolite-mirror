#!/bin/sh
basedir=`dirname "$0"`

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../maxmind-geolite-mirror/bin/maxmind-geolite-mirror" "$@"
  ret=$?
else 
  node  "$basedir/../maxmind-geolite-mirror/bin/maxmind-geolite-mirror" "$@"
  ret=$?
fi
exit $ret
